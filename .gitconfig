[user]
    name = norville
    email = ing.norville@gmail.com
[core]
    editor = vim
    excludesfile = ~/.bdf.cfg/gitignore_global
[init]
    defaultBranch = main
[status]
    submoduleSummary = true
    showUntrackedFiles = normal
[color]
    ui = auto
[filter "lfs"]
    clean = git-lfs clean %f
    smudge = git-lfs smudge %f
    required = true
